    "web3"
  ],
  "author": "henrywfyeung",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/didhubdev/didhub-sdk/issues"
  },
  "homepage": "https://github.com/didhubdev/didhub-sdk#readme",
  "dependencies": {
    "@opensea/seaport-js": "^1.3.0",
    "cross-fetch": "^4.0.0",
    "ethers": "5.6.1",
    "ts-node": "^10.4.1"
  },
  "devDependencies": {
    "@types/node": "^16.7.13",
    "typechain": "^6.0.5",
    "typescript": "^5.0.4"
  }
