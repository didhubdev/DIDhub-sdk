{
  "name": "@didhubdev/sdk",
  "version": "0.1.83",
  "description": "The didhub sdk for interacting with the didhub smart contracts.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "build": "npm run clean && tsc",
